{
  "name": "tmp-minimal-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "ONE": "echo 'APP ONE'",
    "one:dev": "nodemon -r tsconfig-paths/register src/app_one/main.ts",
    "one:prod": "node dist/app_one.prod.js",
    "one:build:dev": "webpack --env APP_BUILD_CONF=./src/app_one/_build.ts webpack --mode development",
    "one:build": "webpack --env APP_BUILD_CONF=./src/app_one/_build.ts --mode production",
    "TWO": "echo 'APP TWO'",
    "two:dev": "nodemon -r tsconfig-paths/register src/app_two/main.ts",
    "two:prod": "node dist/app_two.prod.js",
    "two:build:dev": "webpack --env APP_BUILD_CONF=./src/app_two/_build.ts --mode development",
    "two:build": "webpack --env APP_BUILD_CONF=./src/app_two/_build.ts --mode production",
    "": "",
    "gen-grpc-types": "npx proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=src/grpc/ grpc/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.10",
    "@types/fs-extra": "^11.0.1",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "cron": "^2.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.0",
    "@types/webpack-node-externals": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "nodemon": "^2.0.20",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0"
  },
  "author": "Artyom Silantiev",
  "license": "ISC"
}
