{
  "name": "tmp-minimal-app",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "module": "commonjs",
  "scripts": {
    "ONE": "echo 'APP ONE'",
    "one:dev": "nodemon -r tsconfig-paths/register src/app_one/main.ts",
    "one:prod": "node dist/app_one.min.js",
    "one:build:dev": "ts-node -r tsconfig-paths/register ./rollup.ts --build_conf=./src/app_one/_build.ts --mode=development",
    "one:build": "ts-node -r tsconfig-paths/register ./rollup.ts --build_conf=./src/app_one/_build.ts --mode=production",
    "TWO": "echo 'APP TWO'",
    "two:dev": "nodemon -r tsconfig-paths/register src/app_two/main.ts",
    "two:prod": "node dist/app_two.min.js",
    "two:build:dev": "ts-node -r tsconfig-paths/register ./rollup.ts --build_conf=./src/app_two/_build.ts --mode=development",
    "two:build": "ts-node -r tsconfig-paths/register ./rollup.ts --build_conf=./src/app_two/_build.ts --mode=production",
    "": "",
    "gen-grpc-types": "npx proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=src/grpc/ grpc/*.proto"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "cron": "^2.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.0",
    "@grpc/grpc-js": "^1.8.10",
    "@types/fs-extra": "^11.0.1",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "nodemon": "^2.0.21",
    "rollup": "^3.19.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "yargs": "^17.7.1"
  },
  "author": "Artyom Silantiev",
  "license": "ISC"
}
